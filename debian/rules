#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

# This has to be exported to make some magic below work.
export DH_OPTIONS

override_dh_auto_build:
	export HOME=${HOME} && mvn -P mesos -Duser.home=/tmp -Dhadoop.version=2.6.0 -DskipTests=true package

%:
	dh $@ 
